rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    // Allow users to read/write their own analysis files
    match /analyses/{userId}/{fileName} {
      allow read, write: if request.auth != null;
    }
    
    // Deny all other access
    match /{allPaths=**} {
      allow read, write: if false;
    }
  }
}

